{
    "name": "codica-studio/welcomeemail",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codica-studio/welcomeemail",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/notifications": "^8.0",
        "illuminate/queue": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/test-time": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WelcomeNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\WelcomeNotification\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WelcomeNotification\\WelcomeNotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
